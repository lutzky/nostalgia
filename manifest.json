{
  "manifest_version": 2,

  "name": "Nostalgia",
  "description": "Open least-recently-accessed bookmark in a tab",
  "version": "0.2",

  "permissions": [
    "bookmarks", "history"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "icons": { "128": "icon128.png" },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "options_page": "options.html"
}
